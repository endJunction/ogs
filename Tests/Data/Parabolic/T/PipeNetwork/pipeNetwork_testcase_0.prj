<?xml version="1.0" encoding="ISO-8859-1"?>
<OpenGeoSysProject>
    <mesh>pipeNetwork_testcase_0.vtu</mesh>
    <geometry>pipeNetwork_testcase_0.gml</geometry>
    <processes>
        <process>
            <name>pipeNetwork_testcase_0</name>
            <type>PIPE_NETWORK</type>
            <integration_order>2</integration_order>
            <thermal_conductivity>lambda</thermal_conductivity>
            <heat_capacity>c_p</heat_capacity>
            <density>rho</density>
            <process_variables>
                <process_variable>flow_velocity</process_variable>
            </process_variables>
            <secondary_variables>
                <secondary_variable type="static" internal_name="head" output_name="head"/>
            </secondary_variables>
        </process>
    </processes>
    <time_loop>
        <processes>
            <process ref="PipeNetwork">
                <nonlinear_solver>basic_picard</nonlinear_solver>
                <convergence_criterion>
                    <type>DeltaX</type>
                    <norm_type>NORM2</norm_type>
                    <abstol>1.e-6</abstol>
                </convergence_criterion>
                <time_discretization>
                    <type>BackwardEuler</type>
                </time_discretization>
                <output>
                    <variables>
                        <variable> flow_velocity </variable>
                        <variable> head </variable>
                    </variables>
                </output>
                <time_stepping>
                    <type>FixedTimeStepping</type>
                    <t_initial> 0.0 </t_initial>
                    <t_end> 1 </t_end>
                    <timesteps>
                        <pair>
                            <repeat>1</repeat>
                            <delta_t>1</delta_t>
                        </pair>
                    </timesteps>
                </time_stepping>
            </process>
        </processes>
        <output>
            <type>VTK</type>
            <prefix>pipeNetwork_testcase_0</prefix>
            <timesteps>
                <pair>
                    <repeat> 1 </repeat>
                    <each_steps> 1 </each_steps>
                </pair>
            </timesteps>
        </output>
    </time_loop>
    <parameters>
        <parameter>
            <name>diameter</name>
            <type>Constant</type>
            <value>0.3048</value>
        </parameter>
        <parameter>
            <name>roughness</name>
            <type>Constant</type>
            <value>100.0</value>
        </parameter>
        <parameter>
            <name>minorLoss_coef</name>
            <type>Constant</type>
            <value>0</value>
        </parameter>
        <parameter>
            <name>demand_juncID_3</name>
            <type>Constant</type>
            <value>0.06</value>
        </parameter>
        <parameter>
            <name>reservior_head</name>
            <type>Constant</type>
            <value>3.048</value>
        </parameter>
		<parameter>
            <name>p_spatial</name>
            <type>Constant</type>
            <value>1</value>
        </parameter>
		<parameter>
            <name>pump_curve</name>
            <type>CurveScaled</type>
            <curve>pump_curve_1</curve>
            <parameter>p_spatial</parameter>
        </parameter>
        <parameter>
            <name>pump_curve_x-value</name>
            <type>Constant</type>
            <value>5</value>
        </parameter>
		<parameter>
            <name>pump_curve_y-value</name>
            <type>Constant</type>
            <value>10</value>
        </parameter>
		<parameter>
            <name>initial_pipeflow_and_head_random_value</name>
            <type>Constant</type>
            <value>0.5</value>
        </parameter>
    </parameters>
	<curves>
        <curve>
            <name>pump_curve_1</name>
			<coords>0	5</coords>
			<values>
			10	0
			</values>
		</curve>
    </curves>
    <process_variables>
        <process_variable>
            <name>flow_velocity</name>
            <components>1</components>
            <order>1</order>
            <initial_condition>initial_pipeflow_and_head_random_value</initial_condition>
            <boundary_conditions>
                <boundary_condition>
                    <geometrical_set>geometry</geometrical_set>
                    <geometry>reser_juncID_1</geometry>
                    <type>Constant</type>
                    <parameter>reservior_head</parameter>
                </boundary_condition>
            </boundary_conditions>
			<source_terms>
				<source_term>
                    <geometrical_set>geometry</geometrical_set>
                    <geometry>juncID_3</geometry>
                    <type>Constant</type>
                    <parameter>demand_juncID_3</parameter>
                </source_term>
            </source_terms>
        </process_variable>
    </process_variables>
    <nonlinear_solvers>
        <nonlinear_solver>
            <name>basic_newton_raphson</name>
            <type>Newton</type>
            <max_iter>10</max_iter>
            <linear_solver>general_linear_solver</linear_solver>
        </nonlinear_solver>
    </nonlinear_solvers>
    <linear_solvers>
        <linear_solver>
            <name>general_linear_solver</name>
            <lis>-i cg -p jacobi -tol 1e-16 -maxiter 10000</lis>
            <eigen>
                <solver_type>CG</solver_type>
                <precon_type>DIAGONAL</precon_type>
                <max_iteration_step>10000</max_iteration_step>
                <error_tolerance>1e-16</error_tolerance>
            </eigen>
            <petsc>
                <prefix>gw</prefix>
                <parameters>-gw_ksp_type cg -gw_pc_type bjacobi -gw_ksp_rtol 1e-16 -gw_ksp_max_it 10000</parameters>
            </petsc>
        </linear_solver>
    </linear_solvers>
</OpenGeoSysProject>